<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="by.leonovich.notizieportale.domain" auto-import="false" catalog="mysitetonight">
    <class name="Category" table="T_CATEGORY" lazy="false">
        <id name="categoryId" type="java.lang.Long" column="F_PERSON_ID">
            <generator class="increment"/>
        </id>
        <property name="categoryName" column="F_CATEGORY_NAME" type="string"/>

        <set name="newses" table="T_NEWS" inverse="true" lazy="true" fetch="select">
            <key column="F_NEWS_ID" not-null="true"/>
            <one-to-many class="News"/>
        </set>

    </class>
</hibernate-mapping>